# Version
CONFIG_LOCALVERSION="-topaz"

# DT
CONFIG_BUILD_ARM64_APPENDED_DTB_IMAGE=y
CONFIG_BUILD_ARM64_DT_OVERLAY=y

# Pstore
CONFIG_PSTORE=y
CONFIG_PSTORE_CONSOLE=y
CONFIG_PSTORE_PMSG=y
CONFIG_PSTORE_RAM=y

# Xiaomi
CONFIG_MACH_XIAOMI_SDM439=y
CONFIG_MACH_XIAOMI_OLIVES=y
CONFIG_MACH_XIAOMI_PINE=y

# Display
CONFIG_BACKLIGHT_CLASS_DEVICE=y
CONFIG_BACKLIGHT_XIAOMI=y
CONFIG_FB_MSM_MDSS_XIAOMI_LM3697_SUPPORT=y
CONFIG_FB_MSM_MDSS_XIAOMI_WPONIT_ADJUST=y
CONFIG_MFD_TI_LMU=y
CONFIG_REGULATOR_LM363X=y

# Touchscreen
CONFIG_INPUT_TOUCHSCREEN=y
# CONFIG_TOUCHSCREEN_ST is not set
# CONFIG_TOUCHSCREEN_SYNAPTICS_DSX is not set
# CONFIG_TOUCHSCREEN_SYNAPTICS_TCM is not set
CONFIG_TOUCHSCREEN_FT5446=y
CONFIG_TOUCHSCREEN_ITK9881H=y
CONFIG_TOUCHSCREEN_NT36525B=y
CONFIG_TOUCHSCREEN_FT8006S=y
CONFIG_INPUT_TOUCHSCREEN_XIAOMI_OLIVE=y
CONFIG_VITURALSAR=y

# Fingerprint
CONFIG_FPR_FPC=y
CONFIG_FINGERPRINT_GOODIX_GF3208=y

# ION
CONFIG_ION_LEGACY=y

# VDSO
CONFIG_COMPAT_VDSO=y

# Netfilter
CONFIG_IP_NF_TARGET_TTL=y

# WLAN
CONFIG_PRONTO_WLAN=y

# Initramfs
CONFIG_INITRAMFS_IGNORE_SKIP_FLAG=y

# Wireguard
CONFIG_WIREGUARD=y

# CPU freq stats
CONFIG_CPU_FREQ_STAT=y

# CLK
CONFIG_QCOM_CLK_SMD_RPM=n
CONFIG_SDM_GCC_429W=n
CONFIG_SDM_DEBUGCC_429W=n
CONFIG_CLOCK_CPU_SDM=n
CONFIG_SDM_DEBUGCC_439=n
CONFIG_MSM_MDSS_PLL=y
